{

  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ],
    "no-console": "warn"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}

